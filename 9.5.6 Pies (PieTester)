import java.util.ArrayList;

public class PieTester
{
    public static void main(String[] args)
    {
        Pie pumpkin = new PumpkinPie(6, false);
        Pie applePie = new ApplePie(6);
        Pie blueberry = new Pie("Blueberry", 6);

        System.out.println("Pie: " + pumpkin.getType());
        System.out.println("Pie: " + applePie.getType());
        System.out.println("Pie: " + blueberry.getType());
    }
}
