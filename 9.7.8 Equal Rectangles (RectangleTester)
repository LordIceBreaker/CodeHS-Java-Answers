public class RectangleTester
{
    public static void main(String[] args)
    {
        Rectangle rect1 = new Rectangle(5, 4);
        Rectangle rect2 = new Rectangle(5, 3);
        Rectangle rect3 = new Rectangle(10, 2);
        
        // Print all three rectangles
        System.out.println("Rectangle 1: " + rect1);
        System.out.println("Rectangle 2: " + rect2);
        System.out.println("Rectangle 3: " + rect3);

        // Print one true statement comparing rectangles
        System.out.println("Rectangle 1 equals Rectangle 3?: " + rect1.equals(rect3));

        // Print one false statment comparing rectangles
        System.out.println("Rectangle 1 equals Rectangle 2?: " + rect1.equals(rect2));
    }
}
